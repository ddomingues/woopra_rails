source 'https://rubygems.org'

# Specify your gem's dependencies in woopra_rails.gemspec
gemspec

gem 'rails'

group :development do
  gem 'guard'
  gem 'guard-rspec'
  gem 'rb-fsevent'
end

group :test do
  gem 'database_cleaner'
end

group :test, :development do
  gem 'rspec-rails'
  gem 'factory_girl_rails'
  gem 'debugger'
end
